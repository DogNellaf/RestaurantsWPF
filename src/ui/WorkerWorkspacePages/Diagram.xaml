<Window x:Class="ui.WorkerWorkspacePages.Diagram"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ui.WorkerWorkspacePages"
        mc:Ignorable="d"
        Title="Diagram" Height="450" Width="800" WindowState="Maximized">
    <Grid>
        <Button x:Name="exitButton" Content="Назад" HorizontalAlignment="Right" VerticalAlignment="Top" Height="40" Width="77" FontSize="20" IsCancel="True" Click="exitButton_Click" Margin="0,10,10,0"/>
        <Label x:Name="nameLabel" Content="Бухгалтерский отчет" Margin="92,10,92,0" VerticalAlignment="Top" Height="70" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="24"/>
        <Label x:Name="titleLabel" Content="Суммарный заработок на заказах в этом месяце" HorizontalAlignment="Center" Margin="0,85,0,0" VerticalAlignment="Top" Height="29" Width="628" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="16"/>
        <Grid x:Name="GridForChart" Background="LightGray" Margin="0,119,0,0"  />
    </Grid>
</Window>
